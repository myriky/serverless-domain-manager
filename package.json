    "*.ts",
    "*.json",
    "dist/*.js"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-spies": "^1.0.0",
    "@types/mocha": "^5.2.5",
